#: app/caputil.cpp:50
msgid "Extreme — Extraordinary threat to life or property"
msgstr "بالغ الخطورة - تهديد غير عادي للحياة أو الممتلكات"

#: app/caputil.cpp:51
msgid "Severe — Significant threat to life or property"
msgstr "شديد - تهديد كبير على الحياة أو الممتلكات"

#: app/caputil.cpp:52
msgid "Moderate — Possible threat to life or property"
msgstr "متوسط - تهديد محتمل للحياة أو الممتلكات"

#: app/caputil.cpp:53
msgid "Minor — Minimal to no known threat to life or property"
msgstr "بسيط - الحد الأدنى أو عدم وجود تهديد معروف على الحياة أو الممتلكات"

#: app/caputil.cpp:57
msgid "Responsive action should be taken immediately"
msgstr "يجب اتخاذ إجراءات الاستجابة على الفور"

#: app/caputil.cpp:58
msgid "Responsive action should be taken soon"
msgstr "ينبغي اتخاذ إجراء استجابته في أقرب وقت ممكن"

#: app/caputil.cpp:59
msgid "Responsive action be taken in the near future"
msgstr "اتخاذ إجراءات استجابة في المستقبل القريب"

#: app/caputil.cpp:60
msgid "Responsive action is no longer required"
msgstr "لم يعد إجراء الاستجابة مطلوباً"

#: app/caputil.cpp:64
msgid "Occurred or ongoing"
msgstr "حدثت أو لا تزال جارية"

#: app/caputil.cpp:65
msgid "Likely (> 50%)"
msgstr "محتمل (> 50%)"

#: app/caputil.cpp:66
msgid "Possible but not likely (< 50%)"
msgstr "ممكن ولكن غير محتمل (أقل من 50%)"

#: app/caputil.cpp:67
msgid "Not expected to occur"
msgstr "لا يتوقع حدوثها"

