msgid "General emergency and public safety"
msgstr "Emergenza generale e sicurezza pubblica"

#: app/caputil.cpp:76
msgid "Law enforcement, military, homeland and local/private security"
msgstr "Forze dell'ordine, militari, sicurezza nazionale e locale/privata"

#: app/caputil.cpp:77
msgid "Rescue and recovery"
msgstr "Salvataggio e recupero"

#: app/caputil.cpp:78
msgid "Fire"
msgstr "Incendio"

#: app/caputil.cpp:79
msgid "Medical and public health"
msgstr "Medicina e salute pubblica"

