#: app/caputil.cpp:52
msgid "Extreme — Extraordinary threat to life or property"
msgstr ""
"Elevata — Minaccia straordinaria alla sicurezza delle persone o alle cose"

#: app/caputil.cpp:53
msgid "Severe — Significant threat to life or property"
msgstr ""
"Moderata — Minacce significative alla sicurezza delle persone o alle cose"

#: app/caputil.cpp:54
msgid "Moderate — Possible threat to life or property"
msgstr ""
"Ordinaria — Possibile minaccia alla sicurezza delle persone o alle cose"

#: app/caputil.cpp:55
msgid "Minor — Minimal to no known threat to life or property"
msgstr "Minima — Minaccia minima alla sicurezza delle persone o alle cose"

#: app/caputil.cpp:59
msgid "Responsive action should be taken immediately"
msgstr "È necessario adottare misure di risposta immediatamente"

#: app/caputil.cpp:60
msgid "Responsive action should be taken soon"
msgstr "È necessario adottare misure di risposta al più presto"

#: app/caputil.cpp:61
msgid "Responsive action be taken in the near future"
msgstr "È necessario adottare misure di risposta nel prossimo futuro"

#: app/caputil.cpp:62
msgid "Responsive action is no longer required"
msgstr "La misura di risposta non è più necessaria"

#: app/caputil.cpp:66
msgid "Occurred or ongoing"
msgstr "Verificato o in corso"

#: app/caputil.cpp:67
msgid "Likely (> 50%)"
msgstr "Probabile (> 50%)"

#: app/caputil.cpp:68
msgid "Possible but not likely (< 50%)"
msgstr "Possibile ma non probabile (< 50%)"

#: app/caputil.cpp:69
msgid "Not expected to occur"
msgstr "Non previsto accadere"

