#: app/caputil.cpp:53
msgid "Severe — Significant threat to life or property"
msgstr "Grave — Menace importante pour les personnes ou les biens"

#: app/caputil.cpp:54
msgid "Moderate — Possible threat to life or property"
msgstr "Modéré — Menace possible pour les personnes ou les biens"

#: app/caputil.cpp:55
msgid "Minor — Minimal to no known threat to life or property"
msgstr ""
"Mineur — Menace minimale avec aucune menace connue pour les personnes ou les "
"biens"

#: app/caputil.cpp:59
msgid "Responsive action should be taken immediately"
msgstr "Des mesures réactives doivent être prises immédiatement."

#: app/caputil.cpp:60
msgid "Responsive action should be taken soon"
msgstr "Des mesures réactives devraient être prises rapidement."

#: app/caputil.cpp:61
msgid "Responsive action be taken in the near future"
msgstr "Des mesures réactives doivent être prises dans un proche avenir."

#: app/caputil.cpp:62
msgid "Responsive action is no longer required"
msgstr "Une action réactive n'est plus nécessaire."

#: app/caputil.cpp:66
msgid "Occurred or ongoing"
msgstr "Survenu ou en cours"

#: app/caputil.cpp:67
msgid "Likely (> 50%)"
msgstr "Probable (> 50 %)"

#: app/caputil.cpp:68
msgid "Possible but not likely (< 50%)"
msgstr "Possible mais pas probable (< 50%)"

#: app/caputil.cpp:69
msgid "Not expected to occur"
msgstr "Apparition peu probable"

