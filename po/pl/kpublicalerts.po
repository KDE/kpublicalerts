#: app/caputil.cpp:52
msgid "Extreme — Extraordinary threat to life or property"
msgstr "Ekstremalna — Nadzwyczajne zagrożenie życia czy mienia"

#: app/caputil.cpp:53
msgid "Severe — Significant threat to life or property"
msgstr "Znaczna – Znaczące zagrożenie życia czy mienia"

#: app/caputil.cpp:54
msgid "Moderate — Possible threat to life or property"
msgstr "Umiarkowana – Możliwe zagrożenie życia czy mienia"

#: app/caputil.cpp:55
msgid "Minor — Minimal to no known threat to life or property"
msgstr "Mała – Minimalne lub brak zagrożenia życia czy mienia"

#: app/caputil.cpp:59
msgid "Responsive action should be taken immediately"
msgstr "Odpowiednie działanie powinno zostać podjęte natychmiastowo"

#: app/caputil.cpp:60
msgid "Responsive action should be taken soon"
msgstr "Odpowiednie działanie powinno zostać podjęte wkrótce"

#: app/caputil.cpp:61
msgid "Responsive action be taken in the near future"
msgstr "Odpowiednie działanie powinno zostać podjęte w najbliższej przyszłości"

#: app/caputil.cpp:62
msgid "Responsive action is no longer required"
msgstr "Odpowiednie działania nie są już wymagane"

#: app/caputil.cpp:66
msgid "Occurred or ongoing"
msgstr "Wystąpiło lub trwające"

#: app/caputil.cpp:67
msgid "Likely (> 50%)"
msgstr "Prawdopodobne (> 50%)"

#: app/caputil.cpp:68
msgid "Possible but not likely (< 50%)"
msgstr "Możliwe, lecz mało prawdopodobne (< 50%)"

#: app/caputil.cpp:69
msgid "Not expected to occur"
msgstr "Nie oczekuje się wystąpienia"

#: app/caputil.cpp:73
msgid "Geophysical"
msgstr "Geofizyczne"

#: app/caputil.cpp:74
msgid "Meteorological"
msgstr "Meteorologiczne"

#: app/caputil.cpp:75
msgid "General emergency and public safety"
msgstr "Ogólny stan pogotowia i bezpieczeństwo publiczne"

#: app/caputil.cpp:76
msgid "Law enforcement, military, homeland and local/private security"
msgstr "Policja, wojsko, obrona terytorialna oraz lokalna/prywatna ochrona"

#: app/caputil.cpp:77
msgid "Rescue and recovery"
msgstr "Ratownicze"

#: app/caputil.cpp:78
msgid "Fire"
msgstr "Ogień"

#: app/caputil.cpp:79
msgid "Medical and public health"
msgstr "Medyczne i zdrowia publicznego"

#: app/caputil.cpp:80
msgid "Pollution and other environmental event"
msgstr "Zanieczyszczenie i inne zdarzenia środowiskowe"

#: app/caputil.cpp:81
msgid "Transportation"
msgstr "Transport"

#: app/caputil.cpp:82
msgid "Utility, telecommunication, other non-transport infrastructure"
msgstr "Media, telekomunikacja, inna nie-transportowa infrastruktura"

#: app/caputil.cpp:83
msgid ""
"Chemical, biological, radiological, nuclear or high-yield explosive threat "
"or attack"
msgstr ""
"Napad lub zagrożenie chemiczne, biologiczne, radiologiczne, atomowe czy "
"wysokowybuchowe"

#: app/caputil.cpp:84
msgid "Other event"
msgstr "Inne zdarzenia"

#: app/caputil.cpp:88
msgid "Take shelter in place or per instructions."
msgstr "Schroń się w swoim miejscu lub wg instrukcji."

#: app/caputil.cpp:89
msgid "Relocate as instructed."
msgstr "Zmień miejsca wg instrukcji."

#: app/caputil.cpp:90
msgid "Make preparations as per the instructions."
msgstr "Przygotuj się wg instrukcji."

#: app/caputil.cpp:91
msgid "Execute activities identified in the instructions."
msgstr "Wykonaj czynności wymienione w instrukcjach."

#: app/caputil.cpp:92
msgid "Avoid the subject event as per the instructions."
msgstr "Unikaj wydarzenia wg instrukcji."

#: app/caputil.cpp:93
msgid "Attend to information sources as described in the instructions."
msgstr "Skieruj się do źródeł informacji, tak jak opisano w instrukcjach."

#: app/caputil.cpp:94
msgid "Evaluate the information in this message."
msgstr "Oceń wg informacji w tej wiadomości."

#: app/caputil.cpp:95
msgid ""
"The subject event no longer poses a threat or concern and any follow on "
"action is described in the instructions."
msgstr ""
"Wydarzenie nie stanowi już zagrożenia czy obawy, a sposób postępowania jest "
"opisany w instrukcji."

#: app/caputil.cpp:96
msgid "No action recommended."
msgstr "Nie jest zalecane żadne działanie."

